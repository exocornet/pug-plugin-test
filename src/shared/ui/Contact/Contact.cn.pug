-
	function classes(cn) {
		const BLOCK = CN('contact');

		return {
			contact: BLOCK(
				{
					[`${cn?.size}`]: ['h1', 'h2','h3', 'h4'].includes(cn?.size),
					'text-indent': !!cn?.TEXT_INDENT || !!cn?.textIndent
				},
				[
					cn?.textAlign,
					cn?.margin,
					cn?.position,
					cn?.width
				]
			),
			contactLabel: BLOCK('label', {
				'top-0': !!cn?.top0Footnote,
				'top-26': !!cn?.top26Footnote,
			}, [cn?.positionFootnote, 'mb-12@xs'])
		}
	}
